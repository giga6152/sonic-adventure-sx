if gamepad_button_check_pressed(0,gp_padr) //or gamepad_axis_value (0, gp_axislh) &gt; 0.7
    keyboard_key_press(vk_right)
else if gamepad_button_check_released(0,gp_padr)
    keyboard_key_release(vk_right)

