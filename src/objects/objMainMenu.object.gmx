{
    switch option
    {
        case 0: //Start the game
            alarm[0] = 40;
            instance_create (0, 0, objFade)
        break;
        
        case 1: //Help
            alarm[1] = 40;
            instance_create (0, 0, objFade);
        break;
        
        case 2: //Quit the game
            game_end();
        break;
    }
}
