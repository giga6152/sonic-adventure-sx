{
    if !playing
    {
        playing = true;
        paused = false;
        if audio_is_paused (track[trackID]) audio_resume_sound (track[trackID]);
        else
        {
            audio_stop_all();
            audio_play_sound (track[trackID], 10, true);
        }
    }
    else
    {
        playing = false;
        paused = true;
        audio_pause_sound (track[trackID]);
    }
}
if option == 2
{
    paused = false;
    playing = false;
    audio_stop_all();
    audio_play_sound (sndMenuCancel, 10, false);
}
if option == 3
{
    paused = false;
    playing = false;
    audio_stop_all();
    audio_play_sound (sndMenuSelect, 10, false);
    if trackID &lt; maxID trackID += 1;
}
</string>
